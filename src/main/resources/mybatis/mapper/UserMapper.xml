<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jlkj.mapper.UserMapper">
    <select id="queryById" resultType="com.jlkj.entities.User">
        select * from SEC_USER where id = #{id}
    </select>

    <select id="query" resultType="com.jlkj.entities.User">
        select * from SEC_USER
    </select>
</mapper>